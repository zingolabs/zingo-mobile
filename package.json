    "@react-navigation/stack": "^6.3.2",
    "@sayem314/react-native-keep-awake": "^1.1.0",
    "buffer": "^6.0.3",
    "deep-diff": "^1.0.2",
    "deprecated-react-native-prop-types": "^2.3.0",
